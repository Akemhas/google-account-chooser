{
  "manifest_version": 3,
  "name": "Google Account Chooser Redirect",
  "version": "1.1",
  "description": "Automatically open Google Docs/Drive links with account chooser.",
  "permissions": ["storage", "scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["redirector.js"],
      "run_at": "document_start"
    }
  ]
}
